<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners
                android:bottomLeftRadius="50dp"
                android:bottomRightRadius="13dp"
                android:topLeftRadius="50dp"
                android:topRightRadius="50dp" />
            <solid android:color="@color/default_color" />
        </shape>
    </item>
    <item
        android:id="@+id/one"
        android:width="13dp"
        android:height="13dp"
        android:drawable="@drawable/ic_launcher"
        android:gravity="top|left"
        android:left="3dp"
        android:top="3dp" />
    <item
        android:id="@+id/two"
        android:width="13dp"
        android:height="13dp"
        android:drawable="@drawable/ic_launcher"
        android:gravity="top|left"
        android:left="9dp"
        android:top="6dp" />
    <item
        android:id="@+id/three"
        android:width="13dp"
        android:height="13dp"
        android:drawable="@drawable/ic_launcher"
        android:gravity="top|left"
        android:left="3dp"
        android:top="10dp" />
    <item
        android:id="@+id/four"
        android:width="13dp"
        android:height="13dp"
        android:drawable="@drawable/ic_launcher"
        android:gravity="top|left"
        android:left="10dp"
        android:top="12dp" />
</layer-list>